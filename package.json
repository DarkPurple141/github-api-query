{
  "name": "simple-github-api",
  "version": "1.1.2",
  "description": "A wrapper for the github api to ease requesting files, directory information from a repository",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha test/*.js",
    "coverage": "nyc report --reporter=html --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarkPurple141/github-api-query.git"
  },
  "keywords": [
    "github",
    "api",
    "query",
    "axios"
  ],
  "author": "Alex Hinds <alex.hinds141@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DarkPurple141/github-api-query/issues"
  },
  "homepage": "https://github.com/DarkPurple141/github-api-query#readme",
  "dependencies": {
    "atob": "^2.1.1",
    "axios": "^0.17.1"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**",
      "**/node_modules/**",
      "example/**"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
